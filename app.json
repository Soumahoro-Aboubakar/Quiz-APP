{
  "expo": {
    "name": "Toqetoqe",
    "slug": "Toqetoqe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoApp.jfif",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoApp.jfif",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoApp.jfif",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "build email used"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "react-native-google-mobile-ads": {
      "android_app_id": "PUT YOUR ANDROID ADS ID",
      "ios_app_id": "PUT YOUR ANDROID ADS ID"
    },
    "extra": {
      "eas": {
        "projectId": "build ID"
      }
    }
  }
}
